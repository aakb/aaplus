<table class="table table-condensed table-header-rotated table-hover">
  <thead>
  <tr>
    {{ helper.header('appbundle.tiltagdetail.tilvalgt') }}
    {{ helper.header('appbundle.solcelletiltagdetail.anlaegsstoerrelseKWp') }}
    {{ helper.header('appbundle.solcelletiltagdetail.produktionKWh') }}
    {{ helper.header('appbundle.solcelletiltagdetail.tilNettetPct') }}
    {{ helper.header('appbundle.solcelletiltagdetail.tilEgetForbrugPct') }}
    {{ helper.header('appbundle.solcelletiltagdetail.egetForbrugAfProduktionenKWh') }}
    {{ helper.header('appbundle.solcelletiltagdetail.produktionTilNettetKWh') }}
    {{ helper.header('appbundle.solcelletiltagdetail.forringetYdeevnePrAar') }}
    {{ helper.header('appbundle.solcelletiltagdetail.inverterskift1Aar') }}
    {{ helper.header('appbundle.solcelletiltagdetail.inverterskift2Aar') }}
    {{ helper.header('appbundle.solcelletiltagdetail.prisForNyInverterKr') }}
    {{ helper.header('appbundle.solcelletiltagdetail.salgsprisFoerste10AarKrKWh') }}
    {{ helper.header('appbundle.solcelletiltagdetail.salgsprisEfter10AarKrKWh') }}
    {{ helper.header('appbundle.solcelletiltagdetail.energiprisstigningPctPrAar') }}
    {{ helper.header('appbundle.solcelletiltagdetail.investeringKr') }}
    {{ helper.header('appbundle.solcelletiltagdetail.screeningOgProjekteringKr') }}
    {{ helper.header('appbundle.solcelletiltagdetail.driftPrAarKr') }}
    {{ helper.header('appbundle.solcelletiltagdetail.omkostningTilMaalerKr') }}
    {{ helper.header('appbundle.solcelletiltagdetail.raadighedstarifKr') }}
    {{ helper.header('appbundle.solcelletiltagdetail.totalDriftomkostningerPrAar') }}
    {{ helper.header('appbundle.solcelletiltagdetail.simpelTilbagebetalingstidAar') }}
    {{ helper.header('appbundle.solcelletiltagdetail.nutidsvaerdiSetOver15AarKr') }}
    {{ helper.header() }}
  </tr>
  <tr>
    {{ helper.header_unit() }}
    {{ helper.header_unit('appbundle.tiltagdetail.tilvalgt') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.anlaegsstoerrelseKWp') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.produktionKWh') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.tilNettetPct') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.tilEgetForbrugPct') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.egetForbrugAfProduktionenKWh') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.produktionTilNettetKWh') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.forringetYdeevnePrAar') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.inverterskift1Aar') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.inverterskift2Aar') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.prisForNyInverterKr') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.salgsprisFoerste10AarKrKWh') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.salgsprisEfter10AarKrKWh') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.energiprisstigningPctPrAar') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.investeringKr') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.screeningOgProjekteringKr') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.driftPrAarKr') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.omkostningTilMaalerKr') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.raadighedstarifKr') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.totalDriftomkostningerPrAar') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.simpelTilbagebetalingstidAar') }}
    {{ helper.header_unit('appbundle.solcelletiltagdetail.nutidsvaerdiSetOver15AarKr') }}
    {{ helper.header_unit() }}
  </tr>
  </thead>
  <tbody>
  {% for detail in entity.details %}
    <tr>
      <td>
        {{ block('tiltag_detail_edit') }}
      </td>
      <td>{{ detail.tilvalgt | b2icon }}</td>
      <td>{{ detail.anlaegsstoerrelseKWp | format_decimal }}</td>
      <td>{{ detail.produktionKWh | format_decimal }}</td>
      <td>{{ detail.tilNettetPct | format_percent }}</td>
      <td>{{ detail.tilEgetForbrugPct | format_percent }}</td>
      <td>{{ detail.egetForbrugAfProduktionenKWh | format_decimal }}</td>
      <td>{{ detail.produktionTilNettetKWh | format_decimal }}</td>
      <td>{{ detail.forringetYdeevnePrAar | format_decimal }}</td>
      <td>{{ detail.inverterskift1Aar | format_integer }}</td>
      <td>{{ detail.inverterskift2Aar | format_integer }}</td>
      <td>{{ detail.prisForNyInverterKr | format_decimal }}</td>
      <td>{{ detail.salgsprisFoerste10AarKrKWh | format_decimal }}</td>
      <td>{{ detail.salgsprisEfter10AarKrKWh | format_decimal }}</td>
      <td>{{ detail.energiprisstigningPctPrAar | format_decimal }}</td>
      <td>{{ detail.investeringKr | format_decimal }}</td>
      <td>{{ detail.screeningOgProjekteringKr | format_decimal }}</td>
      <td>{{ detail.driftPrAarKr | format_decimal }}</td>
      <td>{{ detail.omkostningTilMaalerKr | format_decimal }}</td>
      <td>{{ detail.raadighedstarifKr | format_decimal }}</td>
      <td>{{ detail.totalDriftomkostningerPrAar | format_decimal }}</td>
      <td>{{ detail.simpelTilbagebetalingstidAar | format_integer }}</td>
      <td>{{ detail.nutidsvaerdiSetOver15AarKr | format_decimal }}</td>
      <td></td>
    </tr>
  {% endfor %}
  </tbody>
</table>