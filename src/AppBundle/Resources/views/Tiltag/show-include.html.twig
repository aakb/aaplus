<div class="row">
  <div class="col-xs-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <ul class="list-inline">
          <li>{{ entity.tilvalgtAfRaadgiver | b2icon }} {{ 'appbundle.tiltag.tilvalgt' | trans }} {{ 'appbundle.tiltag.tilvalgtAfRaadgiver' | trans }}</li>
          <li>{{ entity.tilvalgtAfAaPlus | b2icon }} {{ 'appbundle.tiltag.tilvalgt' | trans }} {{ 'appbundle.tiltag.tilvalgtAfAaPlus' | trans }}</li>
          <li>{{ entity.tilvalgtAfMagistrat | b2icon }} {{ 'appbundle.tiltag.tilvalgt' | trans }} {{ 'appbundle.tiltag.tilvalgtAfMagistrat' | trans }}</li>
        </ul>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-xs-12 col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">{{ 'appbundle.tiltag.besparelsesoverblik' | trans }}</h3>
      </div>
      <table class="table">
        <tbody>
        <tr>
          <td>{{ 'appbundle.tiltag.varmebesparelseGUF' | trans }}</td>
          <td class="text-right" class="text-right">{{ entity.varmebesparelseGUF | number_format }}</td>
          <td>{{ 'appbundle.tiltag.varmebesparelseGUF.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.varmebesparelseGAF' | trans }}</td>
          <td class="text-right" class="text-right">{{ entity.varmebesparelseGAF | number_format }}</td>
          <td>{{ 'appbundle.tiltag.varmebesparelseGAF.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.elbesparelse' | trans }}</td>
          <td class="text-right">{{ entity.elbesparelse | number_format }}</td>
          <td>{{ 'appbundle.tiltag.elbesparelse.unit' | trans }}</td>
        </tr>
        {% if entity.yderligereBesparelse is defined %}
        <tr>
          <td>{{ 'appbundle.tiltag.yderligereBesparelse' | trans }}</td>
          <td class="text-right">{{ entity.yderligereBesparelse | number_format }}</td>
          <td>{{ 'appbundle.tiltag.yderligereBesparelse.unit' | trans }}</td>
        </tr>
        {% endif %}
        </tbody>
        <tfoot>
        <tr>
          <th>{{ 'appbundle.tiltag.samletEnergibesparelse' | trans }}</th>
          <th class="text-right">{{ entity.samletEnergibesparelse | number_format }}</th>
          <th>{{ 'appbundle.tiltag.samletEnergibesparelse.unit' | trans }}</th>
        </tr>
        <tr>
          <td>{{ 'common.risikovurdering' | trans }}: {{ 'appbundle.tiltag.risikovurderingAendringIBesparelseFaktor' | trans }}</td>
          <td class="text-right">{{ entity.risikovurderingAendringIBesparelseFaktor | format_percent_nounit }}</td>
          <td>{{ 'appbundle.tiltag.risikovurderingAendringIBesparelseFaktor.unit' | trans }}</td>
        </tr>
        <tr>
          <td>
            <small>{{ 'appbundle.tiltag.besparelseDriftOgVedligeholdelse' | trans }}</small>
          </td>
          <td class="text-right">
            <small>{{ entity.besparelseDriftOgVedligeholdelse | number_format }}</small>
          </td>
          <td>
            <small>{{ 'appbundle.tiltag.besparelseDriftOgVedligeholdelse.unit' | trans }}</small>
          </td>
        </tr>
        <tr>
          <th>{{ 'appbundle.tiltag.samletCo2besparelse' | trans }}</th>
          <th class="text-right">{{ entity.samletCo2besparelse | number_format }}</th>
          <th>{{ 'appbundle.tiltag.samletCo2besparelse.unit' | trans }}</th>
        </tr>
        </tfoot>
      </table>
    </div>
  </div>
  <div class="col-xs-12 col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">{{ 'appbundle.tiltag.oekonomioverblik' | trans }}</h3>
      </div>
      <table class="table">
        <tbody>
        <tr>
          <td>{{ 'appbundle.tiltag.anlaegsInvestering' | trans }}</td>
          <td class="text-right">
            {% if entity.anlaegsInvesteringBeregnet != null and entity.anlaegsInvesteringBeregnet != entity.anlaegsInvestering %}
              <span class="aaplus-overwritten-value"><span class="aaplus-calculated-value">({{ 'common.calculated_value' | trans}}: {{ entity.anlaegsInvesteringBeregnet | number_format }})</span> <span class="aaplus-note-marker"></span>
                {{ entity.anlaegsInvestering | number_format }}
              </span>
            {% else %}
              {{ entity.anlaegsInvestering | number_format }}
            {% endif %}
          </td>
          <td>{{ 'appbundle.tiltag.anlaegsInvestering.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.opstartsomkostninger' | trans }}</td>
          <td class="text-right">{{ entity.opstartsomkostninger | number_format }}</td>
          <td>{{ 'appbundle.tiltag.opstartsomkostninger.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.aaplusInvestering' | trans }}</td>
          <td class="text-right">{{ entity.aaplusInvestering | number_format }}</td>
          <td>{{ 'appbundle.tiltag.aaplusInvestering.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'common.risikovurdering' | trans }}: {{ 'appbundle.tiltag.risikovurderingOekonomiskKompenseringIftInvesteringFaktor' | trans }}</td>
          <td class="text-right">{{ entity.risikovurderingOekonomiskKompenseringIftInvesteringFaktor | format_percent_nounit }}</td>
          <td>{{ 'appbundle.tiltag.risikovurderingOekonomiskKompenseringIftInvesteringFaktor.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.genopretning' | trans }}</td>
          <td class="text-right">{{ entity.genopretning | number_format }}</td>
          <td>{{ 'appbundle.tiltag.genopretning.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.modernisering' | trans }}</td>
          <td class="text-right">{{ entity.modernisering | number_format }}</td>
          <td>{{ 'appbundle.tiltag.modernisering.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.simpelTilbagebetalingstidAar' | trans }}</td>
          <td class="text-right">{{ entity.simpelTilbagebetalingstidAar | number_format }}</td>
          <td>{{ 'appbundle.tiltag.simpelTilbagebetalingstidAar.unit' | trans }}</td>
        </tr>
        <tr>
          <td>{{ 'appbundle.tiltag.nutidsvaerdiSetOver15AarKr' | trans }}</td>
          <td class="text-right">{{ entity.nutidsvaerdiSetOver15AarKr | number_format }}</td>
          <td>{{ 'appbundle.tiltag.nutidsvaerdiSetOver15AarKr.unit' | trans }}</td>
        </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

